{
  "name": "browser-monkey",
  "version": "2.7.2",
  "description": "reliable dom testing",
  "main": "index.js",
  "scripts": {
    "test": "standard && yarn test-browser-compatibility && yarn electron-mocha && yarn karma",
    "karma": "karma start --single-run",
    "electron-mocha": "electron-mocha -r @babel/register --renderer test/*Spec.js test/**/*Spec.js",
    "test-browser-compatibility": "browserify ./index.js > /dev/null",
    "update-readme-example-links": "node ./docs/update-readme-example-links.js"
  },
  "author": "Tim Macfarlane <timmacfarlane@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "2vdom": "^0.2.0",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "angular": "^1.7.5",
    "babelify": "^10.0.0",
    "browserify": "16.2.3",
    "browserify-optional": "^1.0.1",
    "codesandbox": "^1.3.8",
    "detect-node": "^2.0.4",
    "electron": "^4.0.1",
    "electron-mocha": "^6.0.4",
    "express": "^4.16.4",
    "httpism": "^3.11.0",
    "hyperdom": "^0.13.0",
    "karma": "3.1.4",
    "karma-browserify": "^6.0.0",
    "karma-browserstack-launcher": "https://github.com/karma-runner/karma-browserstack-launcher",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-env-preprocessor": "0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ievms": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "5.2.0",
    "must": "^0.13.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "server-destroy": "^1.0.1",
    "standard": "^12.0.1",
    "vdom-query": "https://github.com/featurist/vdom-query",
    "virtual-dom": "^2.1.1",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "array.prototype.find": "^2.0.4",
    "chai": "4.2.0",
    "debug-es5": "^4.1.0",
    "deep-equal": "1.0.1",
    "error-stack-parser": "2.0.2",
    "global": "^4.3.2",
    "hobostyle": "1.0.0",
    "jquery": "^3.3.1",
    "lowscore": "^1.12.1",
    "object-inspect": "1.6.0",
    "trytryagain": "1.2.0"
  },
  "standard": {
    "env": [
      "mocha",
      "browser"
    ],
    "ignore": [
      "docs/codesandbox/**/*"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-optional"
    ]
  },
  "keywords": [
    "dom",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/featurist/browser-monkey.git"
  },
  "bugs": {
    "url": "https://github.com/featurist/browser-monkey/issues"
  },
  "homepage": "https://github.com/featurist/browser-monkey"
}
